        "selector": {
          "name": "fh-mbaas"
        },
        "template": {
          "metadata": {
            "labels": {
              "name": "fh-mbaas"
            }
          },
          "spec": {
            "containers": [
              {
                "name": "fh-mbaas",
                "image": "${FH_MBAAS_IMAGE}:${FH_MBAAS_IMAGE_VERSION}",
                "ports": [
                  {
                    "containerPort": 8080
                  }
                ],
                "env": [
                  {
                    "name": "MBAAS_NAMESPACE",
                    "valueFrom": {
                      "fieldRef": {
                        "fieldPath": "metadata.namespace"
                      }
                    }
                  },
                  {
                    "name": "FH_CLUSTER",
                    "value": "${FH_CLUSTER}"
                  },
                  {
                    "name": "FH_MBAASID",
                    "value": "${FH_MBAASID}"
                  },
                  {
                    "name": "FHMBAAS_KEY",
                    "value": "${FHMBAAS_KEY}"
                  },
                  {
                    "name": "MONGODB_REPLICA_NAME",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "mongo-config",
                        "key": "mongodb-replica-name"
                      }
                    }
                  },
                  {
                    "name": "MONGODB_FHMBAAS_USER",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "mongo-config",
                        "key": "mongodb-fhmbaas-user"
                      }
                    }
                  },
                  {
                    "name": "MONGODB_FHMBAAS_PASSWORD",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "mongo-config",
                        "key": "mongodb-fhmbaas-password"
                      }
                    }
                  },
                  {
                    "name": "MONGODB_FHMBAAS_DATABASE",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "mongo-config",
                        "key": "mongodb-fhmbaas-database"
                      }
                    }
                  },
                  {
                    "name": "MONGODB_ADMIN_PASSWORD",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "mongo-config",
                        "key": "mongodb-admin-password"
                      }
                    }
                  },
                  {
                    "name": "MONGODB_FORM_PASSWORD",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "mongo-config",
                        "key": "mongodb-form-password"
                      }
                    }
                  },
                  {
                    "name": "FH_METRICS_API_KEY",
                    "description": "API Key for calling fh-metrics",
                    "value": "${FH_METRICS_API_KEY}"
                  },
                  {
                    "name": "FH_MESSAGING_API_KEY",
                    "description": "API Key for calling fh-messaging",
                    "value": "${FH_MESSAGING_API_KEY}"
                  },
                  {
                    "name": "FH_STATSD_API_KEY",
                    "description": "API Key for calling fh-statsd",
                    "value": "${FH_STATSD_API_KEY}"
                  },
                  {
                    "name": "FH_LOG_LEVEL",
                    "value": "${FH_DEFAULT_LOG_LEVEL}"
                  },
                  {
                    "name": "HTTPS_PROXY",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "node-proxy",
                        "key": "https-proxy"
                      }
                    }
                  },
                  {
                    "name": "HTTP_PROXY",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "node-proxy",
                        "key": "http-proxy"
                      }
                    }
                  },
                  {
                    "name": "NO_PROXY",
                    "valueFrom": {
                      "configMapKeyRef": {
                        "name": "node-proxy",
                        "key": "no-proxy"
                      }
                    }
                  }
                ],
                "resources": {
                  "limits": {
                    "cpu": "800m",
                    "memory": "800Mi"
                  },
                  "requests": {
                    "cpu": "200m",
                    "memory": "200Mi"
                  }
                },
                "livenessProbe": {
                  "httpGet": {
                    "path": "/sys/info/ping",
                    "port": 8080,
                    "scheme": "HTTP"
                  },
                  "initialDelaySeconds": 55,
                  "timeoutSeconds": 5,
                  "periodSeconds": 60,
                  "successThreshold": 1,
                  "failureThreshold": 2
                },
                "readinessProbe": {
                  "httpGet": {
                    "path": "/sys/info/ping",
                    "port": 8080,
                    "scheme": "HTTP"
                  },
                  "timeoutSeconds": 5,
                  "periodSeconds": 10,
                  "successThreshold": 1,
                  "failureThreshold": 1
                },
                "imagePullPolicy": "IfNotPresent"
              }
            ],
            "volumes": [
              {
                "name": "node-proxy",
                "configMap": {
                  "name": "node-proxy"
                }
              }
            ]
          }
        }
      }
    },
