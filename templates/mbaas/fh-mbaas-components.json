      "spec": {
        "ports": [
          {
            "name": "tcp",
            "protocol": "TCP",
            "port": 8080,
            "targetPort": 8080
          },
          {
            "name": "udp",
            "protocol": "UDP",
            "port": 8081,
            "targetPort": 8081
          }
        ],
        "selector": {
          "name": "fh-statsd"
        }
      }
